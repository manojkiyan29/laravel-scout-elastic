{
  "name": "manojkiyan/laravel-scout-elastic",
  "description": "Elastic Driver for Laravel Scout",
  "license": "MIT",
  "authors": [
    {
      "name": "Manoj Kiyan",
      "email": "manojkiyan@wwe.com"
    }
  ],
  "keywords": [
    "laravel",
    "scout",
    "elasticsearch",
    "elastic"
  ],
  "require": {
    "php": "^7.2|^8.0|^8.1",
    "laravel/scout": "^9.3"
  },
  "require-dev": {
    "elasticsearch/elasticsearch": "^7.9",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "suggest": {
    "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^7.9.0)."
  },
  "autoload": {
    "psr-4": {
      "Manojkiyan\\LaravelScoutElastic\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Manojkiyan\\LaravelScoutElastic\\LaravelScoutElasticProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
